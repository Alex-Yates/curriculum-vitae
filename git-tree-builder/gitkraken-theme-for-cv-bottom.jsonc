{
  "meta": {
    "name": "theme-for-cv-bottom",
    "scheme": "light" // must be "light" or "dark"
  },
  "themeValues": {
    // values applied to the entire app
    "root": {
      "red": "#D9413D",
      "orange": "#DE9B43",
      "yellow": "#ECB91C",
      "green": "#5CB85C",
      "blue": "#4D88FF",
      "ltblue": "#5BC0DE",
      "app__bg0": "#FFFFFF",
      "toolbar__bg0": "#242937",
      "toolbar__bg1": "lighten(@toolbar__bg0, 4%)",
      "toolbar__bg2": "lighten(@toolbar__bg1, 6%)",
      "panel__bg0": "#FCFCFC",
      "panel__bg1": "#FAFAFA",
      "panel__bg2": "#F7F7F7",
      "input__bg": "@app__bg0",
      "input-bg-warn-color": "fade(@orange, 60%)",
      "panel-border": "#C6C6C6",
      "section-border": "#DADADA",
      "subtle-border": "#E6E6E6",
      "modal-overlay-color": "rgba(200,200,200,.6)",
      // graph colors
      "graph-color-0": "#00B0F0", // Twitter Blue
      "graph-color-1": "#CC0000", // Redgate Red
      "graph-color-2": "#F6AB25", // DLM Orange
      "graph-color-3": "#c517b6",
      "graph-color-4": "#d90171",
      "graph-color-5": "#cd0101",
      "graph-color-6": "#f25d2e",
      "graph-color-7": "#f2ca33",
      "graph-color-8": "#7bd938",
      "graph-color-9": "#2ece9d",
      // text colors
      // values starting with . aren't added to the CSS, they're just variables
      ".text-color": "#000000",
      "text-selected": "@.text-color",
      "text-normal": "fade(@.text-color, 90%)",
      "text-secondary": "fade(@.text-color, 70%)",
      "text-disabled": "fade(@.text-color, 50%)",
      "text-accent": "#395BBF",
      "text-inverse": "#FFFFFF",
      "text-bright": "@text-inverse",
      // buttons
      "btn-text": "@text-selected",
      "btn-text-hover": "#FFFFFF",
      "default-border": "@text-normal",
      "default-bg": "transparent",
      "default-hover": "transparent",
      "default-border-hover": "@text-selected",
      "primary-border": "@blue",
      "primary-bg": "fade(@blue, 10%)",
      "primary-hover": "fade(@blue, 90%)",
      "success-border": "@green",
      "success-bg": "fade(@green, 10%)",
      "success-hover": "fade(@green, 90%)",
      "warning-border": "@orange",
      "warning-bg": "fade(@orange, 10%)",
      "warning-hover": "fade(@orange, 90%)",
      "danger-border": "@red",
      "danger-bg": "fade(@red, 10%)",
      "danger-hover": "fade(@red, 90%)",
      // states
      "hover-row": "fade(desaturate(@blue, 20%), 10%)",
      "danger-row": "fade(@red, 60%)",
      "selected-row": "fade(desaturate(@blue, 20%), 20%)",
      "warning-row": "fade(@orange, 60%)",
      "droppable": "fade(@yellow, 30%)",
      "drop-target": "fade(@green, 50%)",
      "input--disabled": "fade(#000000, 5%)",
      "link-color": "#005C95",
      "link-color-bright": "#40c5ec",
      "form-control-focus": "@blue",
      // various app elements
      "scroll-thumb-border": "lighten(@toolbar__bg0, 50%)",
      "scroll-thumb-bg": "lighten(@toolbar__bg0, 55%)",
      "scroll-thumb-bg-light": "lighten(@toolbar__bg0, 55%)",
      "wip-status": "fade(@blue, 60%)",
      "card__bg": "#FFFFFF",
      "card-shadow": "#C1C5D7",
      "statusbar__warning-bg": "mixLess(@graph-color-7, @app__bg0, 50%)",
      "label__yellow-color": "#f2ca33",
      "label__light-blue-color": "#15a0bf",
      "label__purple-color": "#c517b6",
      // component states
      "filtering": "fade(@blue, 50%)",
      "soloing": "fade(@orange, 50%)",
      "checked-out": "fade(@green, 30%)",
      "soloed": "fade(@orange, 30%)",
      "filter-match": "fade(@blue, 50%)",
      "toolbar__prompt": "fade(@blue, 20%)",
      "clone__progress":  "fade(@blue, 70%)",
      "verified": "fade(@green, 30%)",
      "unverified": "fade(#000000, 7%)",
      "drop-sort-border": "@green",
      // terminal
      "terminal__repo-name-color": "#00AEFF",
      "terminal__repo-branch-color": "#CD00CD",
      "terminal__repo-tag-color": "#FF0000",
      "terminal__repo-upstream-color": "#00AE00",
      "terminal__background": "#FFFFFF",
      "terminal__cursor": "#000000",
      "terminal__cursorAccent": "#FFFFFF",
      "terminal__foreground": "#000000",
      "terminal__selection": "#E0E9FC",
      "terminal__black": "#000000",
      "terminal__red": "#CD3131",
      "terminal__green": "#0F8845",
      "terminal__yellow": "#B4B418",
      "terminal__blue": "#2472C8",
      "terminal__magenta": "#BC3FBC",
      "terminal__cyan": "#0295BA",
      "terminal__white": "#D2D2D2",
      "terminal__brightBlack": "#525252",
      "terminal__brightRed": "#E43F3F",
      "terminal__brightGreen": "#0DBB75",
      "terminal__brightYellow": "#CCCC00",
      "terminal__brightBlue": "#3B8EEA",
      "terminal__brightMagenta": "#D670D6",
      "terminal__brightCyan": "#1AA9CC",
      "terminal__brightWhite": "#E5E5E5",
      // code editor
      "code-bg": "@app__bg0",
      "code-foreground": "@text-normal",
      "code-blame-color-0": "@graph-color-0",
      "code-blame-color-1": "@graph-color-1",
      "code-blame-color-2": "@graph-color-2",
      "code-blame-color-3": "@graph-color-3",
      "code-blame-color-4": "@graph-color-4",
      "code-blame-color-5": "@graph-color-5",
      "code-blame-color-6": "@graph-color-6",
      "code-blame-color-7": "@graph-color-7",
      "code-blame-color-8": "@graph-color-8",
      "code-blame-color-9": "@graph-color-9",
      "added-line": "fade(@green, 15%)",
      "deleted-line": "fade(@red, 15%)",
      "modified-line": "fade(#000000, 20%)",
      "conflict-info-color": "#15a0bf",
      "conflict-left-border-color": "#15a0bf",
      "conflict-left-color": "fade(@conflict-left-border-color, 25%)",
      "conflict-right-border-color": "#f2ca33",
      "conflict-right-color": "fade(@conflict-right-border-color, 25%)",
      "conflict-output-border-color": "#c517b6",
      "conflict-output-color": "fade(@conflict-output-border-color, 25%)"
    },
    // override specific values just for the toolbar
    "toolbar": {
      "text-selected": "rgba(255,255,255,1)",
      "text-normal": "rgba(255,255,255,.9)",
      "text-secondary": "rgba(255,255,255,.7)",
      "text-disabled": "rgba(255,255,255,.5)",
      "section-border": "rgba(255,255,255,.2)",
      "input__bg": "rgba(0,0,0,.20)",
      "link-color": "#40c5ec",
      "btn-text": "var(--text-normal)"
    },
    // override specific values just for the tabs bar
    "tabsbar": {
      "text-selected": "rgba(255,255,255,1)",
      "text-normal": "rgba(255,255,255,.9)",
      "text-secondary": "rgba(255,255,255,.6)",
      "text-disabled": "rgba(255,255,255,.4)",
      "section-border": "rgba(255,255,255,.2)",
      "input__bg": "rgba(0,0,0,.20)",
      "link-color": "#40c5ec",
      "btn-text": "var(--text-normal)"
    }
  }
}
